{
    "extends": [
        "html-validate:document",
        "html-validate:recommended"
    ],
    "rules": {
        "long-title": "warn",
        "no-autoplay": "off",
        "no-inline-style": [
            "error",
            {
                "allowedProperties": [
                    "display",
                    "background-image"
                ]
            }
        ],
        "require-sri": "off",
        "tel-non-breaking": "off",
        "wcag/h32": "off"
    }
}